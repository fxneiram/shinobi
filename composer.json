{
    "name": "fxneiram/shinobi",
    "description": "Laravel Roles and Permissions",
    "keywords": ["shinobi", "roles", "permissions", "auth", "laravel", "caffeinated"],
    "license": "MIT",
    "authors": [{
        "name": "Shea Lewis",
        "email": "shea.lewis89@gmail.com"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Caffeinated\\Shinobi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinated\\Shinobi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinated\\Shinobi\\ShinobiServiceProvider"
            ],
            "aliases": {
                "Shinobi": "Caffeinated\\Shinobi\\Facades\\Shinobi"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "doctrine/dbal": "^2.10@dev"
    }
}
